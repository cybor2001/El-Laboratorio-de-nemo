//---------------------------------------------------------------------------------------------------------------
//                                                Llamada de las librerías
//---------------------------------------------------------------------------------------------------------------

Adafruit_SSD1306 oled(ANCHO, ALTO, &Wire, OLED_RESET);   // crea objeto

//---------------------------------------------------------------------------------------------------------------
//                                                  Configuración de pines
//---------------------------------------------------------------------------------------------------------------

void setup() {
  
  pinMode(Sensor_Co2,INPUT);                             // Sensor de Co2, MQ135 pin analogico
  pinMode(digTrigger,INPUT);                             // Juego de alimentación digital MQ135 para entrada
  pinMode(led_verde,OUTPUT);                             // conjunto de led para salida
  pinMode(led_azul,OUTPUT);                          // conjunto de led para salida
  pinMode(led_rojo,OUTPUT);                              // conjunto de led para salida
  pinMode(zumbador,OUTPUT);                              // conjunto de led para salida
  Wire.begin();                                          // inicializa bus I2C
  oled.begin(SSD1306_SWITCHCAPVCC, 0x3C);                // inicializa pantalla con direccion 0x3C


//---------------------------------------------------------------------------------------------------------------
//                         LOGOTIPOS DE TUNGSTENO AL INICIO
//---------------------------------------------------------------------------------------------------------------


oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh1, 128, 64, WHITE);
oled.display();
delay(2000);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh2, 128, 64, WHITE);
oled.display();
delay(3000);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh3, 128, 64, WHITE);
oled.display();
delay(900);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh4, 128, 64, WHITE);
oled.display();
delay(900);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh5, 128, 64, WHITE);
oled.display();
delay(900);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh6, 128, 64, WHITE);
oled.display();
delay(900);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh7, 128, 64, WHITE);
oled.display();
delay(900);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh8, 128, 64, WHITE);
oled.display();
delay(900);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh9, 128, 64, WHITE);
oled.display();
delay(2000);

oled.clearDisplay();
oled.drawBitmap(0, 0, bitmap_gaedh10, 128, 64, WHITE);
oled.display();
delay(2000);
}


//---------------------------------------------------------------------------------------------------------------
//                         Programa de datos en pantalla CON CONDICIONES DE DETONANTES POR PPM
//---------------------------------------------------------------------------------------------------------------

 
void loop() {
  
  
int co2now[10];                                         // int para lecturas de co2
int co2raw = 0;                                         // int para el valor bruto de co2
int co2comp = 0;                                        // int para co2 compensado
int co2ppm = 0;                                         // int para ppm calculado
int zzz = 0;                                            // int para promediar
int grafX = 0;                                          // int para el valor x del gráfico
int IDA1 = 350;                                         // 1º nivel del sensor de calidad IDA 1
int IDA2 = 500;                                         // 2º nivel del sensor de calidad IDA 2
int IDA3 = 800;                                         // 3º nivel del sensor de calidad IDA 3
int IDA4 = 1200;                                        // 4º nivel del sensor de calidad IDA 4


//---------------------------------------------------------------------------------------------------------------
//                         LLAMADA DE LECTURA DE SENSORE
//---------------------------------------------------------------------------------------------------------------


   for (int x = 0;x<10;x++){                            // orden de toma de datos de co2
    co2now[x]=analogRead(A0);
    delay(tiempo_lectura);
  } 
  for (int x = 0;x<10;x++){                            // agregar muestras juntas
    zzz=zzz + co2now[x];
    }
  
  co2raw = zzz/10;                                    // dividir muestras por 10
  co2comp = co2raw - co2Zero;                          // obtener valor compensado
  co2ppm = map(co2comp,0,1023,400,5000);               // valor del mapa para los niveles atmosféricos

//---------------------------------------------------------------------------------------------------------------
//                         TEXTO EN PANTALLA OLED
//---------------------------------------------------------------------------------------------------------------

  oled.clearDisplay();                               // limpia pantalla
  oled.setTextColor(WHITE);                          // establece color al unico disponible (pantalla monocromo)
  
  //oled.drawRect (20,13,88,1, WHITE);               // CON ESTO ACTIVAMOS UN SUBRRAYADO DEL TITULO
  oled.setCursor(20, 4);                             // ubica cursor en coordenas X E Y
  oled.setTextSize(1);                               // establece tamano de texto en 1
  oled.print("Medidor de Co2:");                     // 1º Texto en Pantalla

  oled.setCursor(5, 54);                             // ubica cursor en coordenas X E Y
  oled.setTextSize(1);                               // establece tamano de texto en 1
  oled.print("By TunsTeno Grafismo");                // escribe en pantalla el texto

  oled.drawRect (3,52,125,12, WHITE);
  oled.setCursor (15, 22);                           // ubica cursor en coordenas X E Y
  oled.setTextSize(3);                               // establece tamano de texto en 3
  oled.print(co2ppm);                                // Llamada de los datos del sensor de Co2

  oled.setCursor (78, 28);                           // ubica cursor en coordenas X E Y
  oled.setTextSize(2);                               // establece tamano de texto en 2 
  oled.print(" PPM");                                // 1º Texto en Pantalla
  oled.display();                                    // muestra en pantalla todo lo establecido anteriormente

//---------------------------------------------------------------------------------------------------------------
//                         1º nivel de concentración de partículas
//---------------------------------------------------------------------------------------------------------------


  
  if(IDA1>co2ppm){ 

    digitalWrite(zumbador,LOW);                         //Enciende el zumbador   
    digitalWrite(led_rojo,LOW);                          //Apaga el Led Rojo
    digitalWrite(led_azul,LOW);                      //Apaga el Led azul
    digitalWrite(led_verde,HIGH);                         //Enciende el Led Verde
    delay(400);
    digitalWrite(led_verde,LOW);                           //Apaga el Led Rojo
    delay(400);
    digitalWrite(led_verde,HIGH);                           //Apaga el Led Rojo
    delay(400);
    digitalWrite(led_verde,LOW);                           //Apaga el Led Rojo
    delay(400);
    digitalWrite(led_verde,HIGH);                           //Apaga el Led Rojo
    delay(400);
    digitalWrite(led_verde,LOW);                           //Apaga el Led Rojo
    delay(400);
    digitalWrite(led_verde,HIGH);                           //Apaga el Led Rojo
  }
  else{                                                  //Si no...
    digitalWrite(zumbador,LOW);                         //Enciende el zumbador   
    digitalWrite(led_azul,LOW);                       //Apaga el Led azul
    digitalWrite(led_rojo,LOW);                       //Apaga el Led azul
    digitalWrite(led_verde,HIGH);                         //Enciende el Led verde

  }

  if(IDA2<co2ppm){ 
    digitalWrite(zumbador,LOW);                         //Enciende el zumbador   
    digitalWrite(led_azul,LOW);                       //Apaga el Led azul
    //digitalWrite(led_rojo,LOW);                           //Apaga el Led Rojo
    digitalWrite(led_verde,HIGH);                         //Enciende el Led verde
    digitalWrite(led_azul,HIGH);                         //Enciende el Led Verde
    delay(200);
    digitalWrite(led_azul,LOW);                           //Apaga el Led Rojo
    delay(200);
    digitalWrite(led_azul,HIGH);                           //Apaga el Led Rojo
    delay(200);
    digitalWrite(led_azul,LOW);                           //Apaga el Led Rojo
    delay(200);
    digitalWrite(led_azul,HIGH);                           //Apaga el Led Rojo
  }
  
  if(IDA3<co2ppm){ 
    digitalWrite(zumbador,LOW);                         //Enciende el zumbador   
    digitalWrite(led_verde,LOW);                      //Apaga el Led azul
    digitalWrite(led_rojo,HIGH);                          //Apaga el Led Rojo
    digitalWrite(led_azul,HIGH);                         //Enciende el Led Verde
    delay(200);
    digitalWrite(led_azul,LOW);                           //Apaga el Led Rojo
    delay(200);
    digitalWrite(led_azul,HIGH);                           //Apaga el Led Rojo
    delay(200);
    digitalWrite(led_azul,LOW);                           //Apaga el Led Rojo
    delay(200);
    digitalWrite(led_azul,HIGH);                           //Apaga el Led Rojo
  }
  
  if(IDA4<co2ppm){ 
    //digitalWrite(zumbador,LOW);                         //Enciende el zumbador   
    //digitalWrite(led_verde,LOW);                         //Enciende el Led verde
    //digitalWrite(led_rojo,LOW);                           //Apaga el Led Rojo
    //digitalWrite(led_azul,HIGH);                       //Apaga el Led azul
    digitalWrite(led_verde,LOW);                               //Apaga el Led verde
    digitalWrite(led_azul,LOW);                                //Apaga el Led azul
    digitalWrite(led_rojo,HIGH);                               //Enciende el Led Rojo
    digitalWrite(zumbador,HIGH);                           //Enciende el Zumbador
    delay(tiempo_parpadeo);
    digitalWrite(zumbador,LOW);                           //Apaga el Led zumbador
    delay(tiempo_parpadeo);
    digitalWrite(zumbador,HIGH);                           //Enciende el Zumbador
    digitalWrite(led_rojo,LOW);                               //Enciende el Led Rojo
    delay(tiempo_parpadeo);
    digitalWrite(zumbador,LOW);                           //Apaga el Led zumbador
    digitalWrite(led_rojo,HIGH);                               //Enciende el Led Rojo
    delay(tiempo_parpadeo);
    digitalWrite(zumbador,HIGH);                           //Enciende el Zumbador
    digitalWrite(led_rojo,LOW);                               //Enciende el Led Rojo
    delay(tiempo_parpadeo);
    digitalWrite(zumbador,LOW);                           //Apaga el Led zumbador
    digitalWrite(led_rojo,HIGH);                               //Enciende el Led Rojo
   }
      
