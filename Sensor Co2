/*
 * Dióxido de Carbono / anhidrico Carbónico. Partes por Millon
 * CO2 PPM
 * 
 * JMC
 * 12 MAR 2021 // Correcciones de RITE, colores y pitidos
 * 
 * https://www.linkedin.com/in/josecaamano/
 */

/* Aquí anotar datos del rite
* IDA 1..............350ppm    Aire Puro
* IDA 2..............500ppm    Aire con altas concentraciones de partículas.
* IDA 3..............800ppm    Aire con altas concentraciones de contaminantes gaseosos.
* IDA 4..............1.200ppm  Aire con altas concentraciones de contaminates gaseosos y particulas.
*/

//---------------------------------------------------------------------------------------------------------------
//                                                  Librerías Necesarias
//---------------------------------------------------------------------------------------------------------------

#include <Wire.h>                                               //I2C para la pantalla OLED
#include <Adafruit_GFX.h>                                       //Libreria de gráficos para la pantalla OLED
#include <Adafruit_SSD1306.h>                                   //OLED Drives

//---------------------------------------------------------------------------------------------------------------
//                                                   Definicion de los útiles
//---------------------------------------------------------------------------------------------------------------

#define ANCHO 128                                              // Definición en la pantalla del ANCHO por 128
#define ALTO 64                                                // Definición en la pantalla delALTO por 64
#define Sensor_Co2         A0                                  // alimentación analógica de MQ135
#define digTrigger          2                                  // alimentación digital de MQ135
#define co2Zero            55                                  // nivel de CO2 0 calibrado
#define led_verde           7                                  // led de color verde en el pin 7
#define led_azul            8                                  // led de color azul en el pin 8
#define led_rojo            6                                  // led de color rojo en el pin 9
#define zumbador           12                                  // led de color rojo en el pin 9
#define tiempo_lectura    500                                  // Tiempo de la lectura (segundos x 100)
#define tiempo_parpadeo   100                                  // Tiempo de la lectura (segundos x 100)
#define OLED_RESET          4                                  // necesario por la libreria pero no usado

#define GAEDH_BMPWIDTH  128

//---------------------------------------------------------------------------------------------------------------
//                                                Diseño de Logotipo Tungsteno Blanco Sobre negro
//---------------------------------------------------------------------------------------------------------------

#pragma once

#define GAEDH_BMPWIDTH  128

